{
  "expo": {
    "name": "maack",
    "slug": "maack",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "maack",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "platforms": [
      "ios"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSMicrophoneUsageDescription": "Give $(PRODUCT_NAME) permission to use your microphone. Your record wont be shared without your permission."
      }
    },
    "plugins": [
      [
        "expo-asset",
        {
          "assets": [
            "./assets/music/kingbee.jpeg", 
            "./assets/image/forest.png"
          ]
        }
      ],
      "expo-router"
    ],
    "extra": {
      "aiServerHost": "222.98.35.21:80",
      "backendServerHost": "3.36.180.35:8080"
    }
  }
}
